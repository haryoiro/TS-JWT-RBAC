{
  "name": "ts-express-server-template",
  "private": true,
  "description": "This is a modern Express Server template using TypeScript",
  "author": "Haryoiro",
  "license": "ISC",
  "proxy": "http://localhost:8000",
  "scripts": {
    "tsc": "node -r esbuild-register",
    "clean": "node ./node_modules/rimraf/bin.js dist",
    "build": "cross-env NODE_ENV=production yarn clean && yarn tsc && node ./dist/index.js",
    "dev": "cross-env NODE_ENV=development ts-node-dev ./src/index.ts",
    "test": "jest __tests__",
    "typeorm": "ts-node ./node_modules/.bin/typeorm",
    "migration:run": "yarn typeorm migration:run"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.4",
    "@types/uuid": "^8.3.1",
    "axios": "^0.21.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "csurf": "^1.11.0",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-boom": "^3.0.0",
    "express-boom-v2": "^4.2.2",
    "faker": "^5.5.3",
    "helmet": "^4.6.0",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "routing-controllers": "^0.9.0",
    "routing-controllers-openapi": "^3.1.0",
    "sqlite3": "^5.0.2",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.37",
    "typeorm-pagination": "^2.0.3",
    "typescript": "^4.4.3",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@swc/core": "^1.2.101",
    "@swc/helpers": "^0.2.13",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.1",
    "@types/config": "^0.0.39",
    "@types/cors": "^2.8.12",
    "@types/csurf": "^1.11.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.9.1",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/winston": "^2.4.4",
    "config": "^3.3.6",
    "jest": "^27.2.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "typeorm-routing-controllers-extensions": "^0.2.0"
  }
}
